{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/videos/add/page.jsx", "src/apsrc/app/videos/delete/page.jsxc/apsrc/app/videos/edit/[slug]/page.jsxc/apsrc/app/videos/edit/page.jsxc/app/layout.jsx", "src/app/page.jsx", "src/apsrc/app/videos/page.jsx
  "exclude": ["node_modules"]
}
